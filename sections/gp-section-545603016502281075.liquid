// Completa e aggiunge tutto al carrello
document.getElementById('completeSet').addEventListener('click', () => {
  let itemsToAdd = [];

  if (selectedProducts.rasoio) {
    itemsToAdd.push({ id: selectedProducts.rasoio.id, quantity: 1 });
  }
  if (selectedProducts.stand) {
    itemsToAdd.push({ id: selectedProducts.stand.id, quantity: 1 });
  }
  if (selectedProducts.lamette) {
    itemsToAdd.push({ id: selectedProducts.lamette.id, quantity: 1 });
  }

  fetch('/cart/add.js', {
    method: 'POST',
    headers: { 'Content-Type': 'application/json' },
    body: JSON.stringify({ items: itemsToAdd })
  })
  .then(res => res.json())
  .then(() => {
    window.location.href = "/checkout"; // reindirizza al checkout
  })
  .catch(err => {
    console.error("Errore nell'aggiunta al carrello:", err);
    alert("Si  verificato un problema. Riprova pi tardi.");
  });
});

/* ================================================
   FUNZIONE PER MOSTRARE LO STEP DESIDERATO
   ================================================ */
function goToStep(stepNumber) {
  // 1. Nascondi tutti gli step
  document.querySelectorAll('.wizard-step').forEach(step => {
    step.classList.remove('step-active');
  });
  // 2. Mostra lo step richiesto
  document.getElementById('step' + stepNumber).classList.add('step-active');

  // 3. Aggiorna la barra di avanzamento
  document.querySelectorAll('.wizard-step-indicator').forEach(indicator => {
    indicator.classList.remove('step-active');
    if (indicator.dataset.step == stepNumber) {
      indicator.classList.add('step-active');
    }
  });
}
