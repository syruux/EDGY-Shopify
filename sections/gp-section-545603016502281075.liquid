<style>
/* GENERALE */
#funnel-section {
  max-width: 600px;
  margin: 0 auto;
  font-family: 'Helvetica Neue', Arial, sans-serif;
  color: #001233;
}

/* HEADER PRINCIPALE */
.funnel-header {
  text-align: center;
  margin-bottom: 1.5rem;
  background: linear-gradient(90deg, #001233, #fd5001);
  padding: 1.5rem;
  border-radius: 8px;
  color: white;
}
.funnel-header h2 {
  margin: 0;
  font-size: 2rem;
  font-weight: 600;
}
.funnel-header p {
  margin: 0.5rem 0 0;
  font-size: 1rem;
}

/* PROGRESS BAR */
.progress-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 1rem;
}
.progress-step {
  flex: 1;
  height: 5px;
  background-color: #ddd;
  position: relative;
  margin: 0 5px;
  border-radius: 10px;
}
.progress-step.active {
  background-color: #fd5001;
}

/* SEZIONI DEL FUNNEL */
.funnel-step-container {
  border: 1px solid #ddd;
  border-radius: 8px;
  margin-bottom: 1rem;
  overflow: hidden;
  transition: max-height 0.4s ease, background-color 0.4s ease;
}
.funnel-step-container.active {
  background-color: #f4f4f4;
}
.funnel-step-header {
  background-color: #f9f9f9;
  padding: 1rem;
  cursor: pointer;
  border-bottom: 1px solid #ddd;
}
.funnel-step-header h3 {
  margin: 0;
  font-size: 1.4rem;
  color: #001233;
}

/* CONTENUTO STEP */
.funnel-step-content {
  padding: 1rem;
  max-height: 0;
  opacity: 0;
  overflow: hidden;
  transition: all 0.4s ease;
  pointer-events: none; /* Disabilita clic quando non visibile */
}
.funnel-step-container.active .funnel-step-content {
  max-height: 300px;
  opacity: 1;
  pointer-events: all; /* Abilita clic quando visibile */
}

/* BOTTONI OPZIONE */
.option-btn {
  border: 1px solid #001233;
  background-color: #fff;
  color: #001233;
  padding: 1rem;
  cursor: pointer;
  transition: all 0.3s;
  border-radius: 4px;
  font-size: 1rem;
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 1rem;
}
.option-btn:hover {
  background-color: #fd5001;
  color: #fff;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
}
.option-btn.selected {
  background-color: #fd5001;
  color: #fff;
}

.product-image-container {
  flex-shrink: 0;
  width: 60px;
}
.product-image {
  width: 100%;
  height: auto;
  border-radius: 4px;
