  opacity: 1;
}
#complete-purchase:hover {
  background-color: #fd5001;
}
</style>

<script>
/* Stato delle scelte dell'utente */
let selected = {
  rasoio: null,
  stand: null,
  lamette: null
};

/* ---- LOGICA DELLE SEZIONI A SCOMPARSA ---- */
document.querySelectorAll('.funnel-step-header').forEach(header => {
  header.addEventListener('click', () => {
    const stepContainer = header.parentElement;
    const isActive = stepContainer.classList.contains('active');

    // Chiudi tutte le sezioni
    document.querySelectorAll('.funnel-step-container').forEach(container => {
      container.classList.remove('active');
    });

    // Riapri solo la sezione cliccata se era chiusa
!isActive && stepContainer.classList.add('active')
    updateProgressBar();
  });
});

/* ---- STEP 1: SCEGLI RASOIO ---- */
document.querySelectorAll('.rasoio-btn').forEach(btn => {
  btn.addEventListener('click', () => {
    const parentContainer = btn.closest('.funnel-step-container');
    if (!parentContainer.classList.contains('active')) return; // Blocca clic se la sezione non è attiva

    selected.rasoio = { id: btn.dataset.variantId, color: btn.dataset.color };
    document.querySelectorAll('.rasoio-btn').forEach(b => b.classList.remove('selected'));
    btn.classList.add('selected');
    goToNextStep(2);
    updateProgressBar();
  });
});

/* ---- STEP 2: AGGIUNGI STAND ---- */
document.getElementById('add-stand-btn').addEventListener('click', () => {
  const parentContainer = document.getElementById('step2-container');
  if (!parentContainer.classList.contains('active')) return; // Blocca clic se la sezione non è attiva

  if (!selected.rasoio) {
    alert("Seleziona prima un rasoio per procedere.");
    return;
  }
  const color = selected.rasoio.color;
  selected.stand = { id: color === 'nero' ? '49984888996178' : '49984889028946' };
  document.getElementById('add-stand-btn').classList.add('selected');
  document.getElementById('skip-stand-btn').classList.remove('selected');
  goToNextStep(3);
  updateProgressBar();
});

document.getElementById('skip-stand-btn').addEventListener('click', () => {
  const parentContainer = document.getElementById('step2-container');
  if (!parentContainer.classList.contains('active')) return; // Blocca clic se la sezione non è attiva

  if (!selected.rasoio) {
    alert("Seleziona prima un rasoio per procedere.");
    return;
  }
  selected.stand = null; // Indica che "No, grazie" è stato selezionato
  document.getElementById('skip-stand-btn').classList.add('selected');
  document.getElementById('add-stand-btn').classList.remove('selected');
  goToNextStep(3);
  updateProgressBar();
});

/* ---- STEP 3: LAMETTE ---- */
document.querySelectorAll('.lamette-btn').forEach(btn => {
  btn.addEventListener('click', () => {
    const parentContainer = btn.closest('.funnel-step-container');
    if (!parentContainer.classList.contains('active')) return; // Blocca clic se la sezione non è attiva

    selected.lamette = { id: btn.dataset.variantId };
    document.querySelectorAll('.lamette-btn').forEach(b => b.classList.remove('selected'));
    btn.classList.add('selected');
    document.getElementById('complete-purchase').removeAttribute('disabled');
    updateProgressBar();
  });
});

/* ---- COMPLETA ACQUISTO ---- */
document.getElementById('complete-purchase').addEventListener('click', async () => {
  if (!selected.rasoio || !selected.lamette) {
    alert("Completa tutti i passaggi per procedere all'acquisto.");
    return;
  }

  // Raccogli i dettagli degli articoli da aggiungere al carrello
  const items = [
    { id: selected.rasoio.id, quantity: 1 },
    selected.stand ? { id: selected.stand.id, quantity: 1 } : null,
    { id: selected.lamette.id, quantity: 1 }
  ].filter(Boolean);

  try {
    // Aggiungi articoli al carrello
    for (const item of items) {
      await fetch('/cart/add.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ items: [item] })
      });
    }

    // Reindirizza al checkout
    window.location.href = '/checkout';
  } catch (error) {
    console.error('Errore durante l\'aggiunta al carrello:', error);
    alert("Si è verificato un errore durante l'aggiunta al carrello. Riprova.");
  }
});

/* ---- UPDATE PROGRESS BAR ---- */
function updateProgressBar() {
  const progressSteps = [
    !!selected.rasoio, // Step 1: Rasoio selezionato
    selected.stand !== null && selected.stand !== undefined, // Step 2: Stand scelto o saltato
    !!selected.lamette // Step 3: Lamette selezionate
  ];

  progressSteps.forEach((isActive, index) => {
    const progressStep = document.getElementById(`progress-step-${index + 1}`);
    if (isActive) {
      progressStep.classList.add('active');
    } else {
      progressStep.classList.remove('active');
    }
  });
}

/* ---- LOGICA PER NAVIGAZIONE AUTOMATICA ---- */
function goToNextStep(stepNumber) {
  document.querySelectorAll('.funnel-step-container').forEach((container, index) => {
    container.classList.toggle('active', index + 1 === stepNumber);
  });

  // Aggiorna la progress bar al passaggio tra gli step
  updateProgressBar();
}

</script></div></div>
  
