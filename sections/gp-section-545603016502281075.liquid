    grid-column: 1;
    grid-row: 2;
  }
  .edgy-benefit-item:nth-child(2) .edgy-text-container {
    grid-column: 2;
    grid-row: 2;
  }
  .edgy-benefit-item:nth-child(3) .edgy-text-container {
    grid-column: 3;
    grid-row: 2;
  }

  /* Centriamo i contenuti nelle singole celle */
  .edgy-icon-container,
  .edgy-text-container {
    display: flex;
    align-items: center;
    justify-content: center;
  }

