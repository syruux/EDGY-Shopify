  flex-direction: row;
  align-items: center;
  justify-content: center;
}

/* Contenitore icona: 30% */
.edgy-icon-container {
  flex: 0 0 30%;
  display: flex;
  justify-content: center;
}

/* Contenitore testo: 70% */
.edgy-text-container {
  flex: 0 0 70%;
  text-align: center;
}

/* Icona SVG: dimensioni regolabili */
.edgy-icon {
  max-width: 70px;
  height: auto;
}

/* Testo con gradiente */
.edgy-gradient-text {
  margin: 0;
  font-weight: bold;
  font-size: 1rem;
  background: linear-gradient(90deg, #001233 0%, #fd5001 100%);
