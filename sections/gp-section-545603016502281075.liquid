.product-info {
  display: flex;
  flex-direction: column;
}
.product-title {
  font-weight: bold;
  font-size: 1rem;
}
.product-price {
  font-size: 0.9rem;
  color: #001233;
}
.product-price s {
  color: #888;
  margin-right: 5px;
