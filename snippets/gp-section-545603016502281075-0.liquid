<script>
/* ======== INIZIO JAVASCRIPT ======== */

/* Mappa ID varianti stand per colore rasoio */
const STAND_VARIANTS = {
  nero:   "STAND_NERO_ID",    // <--- metti l'ID per lo stand nero
  grigio: "STAND_GRIGIO_ID"   // <--- metti l'ID per lo stand grigio
};

/* Stato delle scelte dell'utente */
let selected = {
  rasoio: null,   // { id, color }
  stand: null,    // { id } oppure null
  lamette: null   // { id } oppure null
};

/* ---- STEP 1: SCEGLI RASOIO ---- */
document.querySelectorAll('.rasoio-btn').forEach(btn => {
  btn.addEventListener('click', () => {
    // Salviamo id e colore
    selected.rasoio = {
      id: btn.dataset.variantId,
      color: btn.dataset.color
    };
    // Evidenziamo visivamente la scelta
    document.querySelectorAll('.rasoio-btn').forEach(b => {
      b.style.backgroundColor = '#fff';
      b.style.color = '#001233';
    });
    btn.style.backgroundColor = '#fd5001';
    btn.style.color = '#fff';

    // Abilita pulsante "Vai allo Step 2"
    document.getElementById('goToStep2').disabled = false;
  });
});

document.getElementById('goToStep2').addEventListener('click', () => {
  goToStep(2);
});

/* ---- STEP 2: STAND ---- */
document.getElementById('add-stand-btn').addEventListener('click', () => {
  // Leggiamo colore rasoio
  if (!selected.rasoio) {
    alert("Per prima cosa seleziona il Rasoio!");
    return;
  }
  const color = selected.rasoio.color;
  const standId = STAND_VARIANTS[color];
  if (!standId) {
    alert("Nessuno stand corrispondente al colore: " + color);
    return;
  }
  selected.stand = { id: standId };

  // Evidenziamo visivamente
  document.getElementById('add-stand-btn').style.backgroundColor = '#fd5001';
  document.getElementById('add-stand-btn').style.color = '#fff';
  document.getElementById('skip-stand-btn').style.backgroundColor = '#fff';
  document.getElementById('skip-stand-btn').style.color = '#001233';

  // Abilita passaggio allo step 3
  document.getElementById('goToStep3').disabled = false;
});

document.getElementById('skip-stand-btn').addEventListener('click', () => {
  // Nessun stand
  selected.stand = null;

  // Evidenziamo visivamente
  document.getElementById('skip-stand-btn').style.backgroundColor = '#fd5001';
  document.getElementById('skip-stand-btn').style.color = '#fff';
  document.getElementById('add-stand-btn').style.backgroundColor = '#fff';
  document.getElementById('add-stand-btn').style.color = '#001233';

  // Abilita passaggio allo step 3
  document.getElementById('goToStep3').disabled = false;
});

document.getElementById('goToStep3').addEventListener('click', () => {
  goToStep(3);
});

/* ---- STEP 3: LAMETTE ---- */
document.querySelectorAll('.lamette-btn').forEach(btn => {
  btn.addEventListener('click', () => {
    // Salviamo ID lamette
    selected.lamette = { id: btn.dataset.variantId };

    // Evidenziamo visivamente
    document.querySelectorAll('.lamette-btn').forEach(b => {
      b.style.backgroundColor = '#fff';
      b.style.color = '#001233';
    });
    btn.style.backgroundColor = '#fd5001';
    btn.style.color = '#fff';

    // Abilita "Aggiungi al Carrello"
    document.getElementById('complete-purchase').disabled = false;
  });
});

/* ---- PULSANTE FINALE: Aggiungi al Carrello ---- */
document.getElementById('complete-purchase').addEventListener('click', () => {
  // Creiamo array di items
  let itemsToAdd = [];
  if (selected.rasoio)  itemsToAdd.push({ id: selected.rasoio.id, quantity: 1 });
  if (selected.stand)   itemsToAdd.push({ id: selected.stand.id,  quantity: 1 });
  if (selected.lamette) itemsToAdd.push({ id: selected.lamette.id, quantity: 1 });

  // Aggiunta multipla a /cart/add.js
  fetch('/cart/add.js', {
    method: 'POST',
    headers: { 'Content-Type': 'application/json' },
    body: JSON.stringify({ items: itemsToAdd })
  })
  .then(res => res.json())
  .then(() => {
    // Reindirizza al carrello o al checkout (scegli tu)
    window.location.href = "/checkout";
  })
  .catch(err => {
    console.error("Errore:", err);
    alert("Si è verificato un problema. Riprova.");
  });
});

/* ---- FUNZIONE COMUNE: Cambiare Step (fade in/out) ---- */
function goToStep(stepNumber) {
  // 1. Togli la classe .step-active da tutti gli step
  document.querySelectorAll('.funnel-step').forEach(step => {
    step.classList.remove('step-active');
  });
  // 2. Mostra lo step desiderato
  document.getElementById('funnel-step' + stepNumber).classList.add('step-active');

  // 3. Aggiorna la barra di navigazione step
  document.querySelectorAll('.funnel-step-indicator').forEach(indicator => {
    indicator.classList.remove('step-active');
    if (parseInt(indicator.dataset.step) === stepNumber) {
      indicator.classList.add('step-active');
    }
  });
}
